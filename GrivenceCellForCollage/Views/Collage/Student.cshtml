@model IEnumerable<GrivenceCellForCollage.Models.CompaintBox>
@{
    Layout = "~/Views/Shared/_StudentLayout.cshtml";
}


<form >
    <table class="table">
        <thead>
        <tr>
          
            <th>
                ComplaintId
            </th>
           
            <th>
               Description
            </th>
            <th>
                Id
            </th>
            <th>
                DateTime
            </th>
            <th>
                Status
            </th>
       </tr>
       </thead>
       <tbody>
            @foreach(var  items in Model)
            {
                <tr>
                    <td>  @items.ComplaintId</td>
                    <td>  @items.Description</td>
                    <td>  @items.Id</td>
                    <td>  @items.Time</td>
                    <td>  @items.status</td>
                </tr>
            }

       </tbody>
    </table>
</form>
